{
  "expo": {
    "name": "PsyPrevenir",
    "slug": "psyprevenir",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "psyprevenir",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0032a0"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0032a0"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PsyPrevenir to access your camera to scan substances."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "openaiApiKey": "YOUR_OPENAI_API_KEY_HERE"
    }
  }
}
